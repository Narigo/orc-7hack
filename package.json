    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "browserify": "^14.3.0",
    "chalk": "^1.1.3",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
