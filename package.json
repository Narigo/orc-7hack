  "scripts": {
    "compile": "babel -q --ignore '*.spec.js' -d lib/ src",
    "dev": "gulp dev",
    "start": "npm run dev",
    "coverage": "nyc npm run test",
    "lint": "eslint src/",
    "lint:watch": "eslint src/ -w",
